<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/cron/update-checker</url>
        <description>Checks if an update of our dataset is available on NOD.</description>
        <schedule>every day 04:00</schedule>
        <timezone>Europe/Paris</timezone>
    </cron>
    <cron>
        <url>/cron/notifier</url>
        <description>Sends email and XMPP notifications</description>
        <schedule>every 2 minutes</schedule>
        <timezone>Europe/Paris</timezone>
    </cron>
</cronentries>
